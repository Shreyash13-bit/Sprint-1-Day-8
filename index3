<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Car Class Example</title>
</head>
<body>

  <script>
    // Define the Car class
    class Car {
      constructor(brand, speed) {
        this.brand = brand;
        this.speed = speed;
      }

      // Method to increase speed by 10
      accelerate() {
        this.speed += 10;
        console.log(`${this.brand} accelerated to ${this.speed} km/h.`);
      }
    }

    // Create a Car object
    const myCar = new Car('Toyota', 50);

    // Call the accelerate method
    myCar.accelerate(); // Output: Toyota accelerated to 60 km/h
    myCar.accelerate(); // Output: Toyota accelerated to 70 km/h
  </script>

</body>
</html>
